{
  "name": "OrderDetail",
  "base": "BaseModel",
  "strict": true,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins":{
    "ReadOnly2": {
      "remoteMethods": true,
      "totalPrice": true,
      "fakeTotalPrice": true
    },
    "Computed2": {
      "properties": {

      }
    }
  },
  "hidden": [],
  "properties": {
    "unitPrice": {
      "type": "number",
      "required": true
    },
